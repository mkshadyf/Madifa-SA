{
  "version": 2,
  "buildCommand": "bash vercel-build.sh",
  "outputDirectory": ".vercel/output",
  "framework": null,
  "regions": ["fra1"],
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}